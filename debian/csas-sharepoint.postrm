#!/bin/sh
set -e

case "$1" in
    remove|purge)
        # Cleanup is handled automatically by dpkg for packaged files
        # No manual cleanup needed since we install actual files, not symlinks
        ;;
    
    upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
        ;;
    
    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

#DEBHELPER#

exit 0